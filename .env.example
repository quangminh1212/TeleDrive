# Telegram Bot Token (từ BotFather)
BOT_TOKEN=your_telegram_bot_token

# Port cho web server (mặc định: 3005)
PORT=3005

# Giới hạn kích thước file (bytes, mặc định: 20MB = 20971520)
MAX_FILE_SIZE=20971520

# Thư mục lưu trữ file (mặc định: uploads)
UPLOAD_DIR=uploads

# Thư mục lưu trữ dữ liệu (mặc định: data)
DATA_DIR=data 