# Telegram API Configuration
# Get these from https://my.telegram.org/apps
API_ID=your_api_id_here
API_HASH=your_api_hash_here

# Phone number for authentication (with country code, e.g., +84123456789)
PHONE_NUMBER=your_phone_number_here

# Session name (can be any string)
SESSION_NAME=teledrive_session

# Default download directory
DOWNLOAD_DIR=./downloads

# Default channel username or ID (optional)
DEFAULT_CHANNEL=@your_channel_username
