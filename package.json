{
  "name": "teledrive",
  "version": "1.0.0",
  "description": "Ứng dụng quản lý và lưu trữ sử dụng Telegram như một giải pháp thay thế cho Google Drive",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "tslint -p tsconfig.json",
    "init-db": "ts-node src/scripts/init-db.ts",
    "clean": "rimraf dist",
    "build:clean": "npm run clean && npm run build",
    "prepare": "npm run build:clean",
    "start:prod": "NODE_ENV=production node dist/index.js"
  },
  "keywords": [
    "telegram",
    "storage",
    "file-management",
    "google-drive-alternative"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.8",
    "multer": "^1.4.4",
    "node-cache": "^5.1.2",
    "telegraf": "^4.7.0",
    "telegram": "^2.12.3",
    "uuid": "^9.0.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.23",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "rimraf": "^5.0.10",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
