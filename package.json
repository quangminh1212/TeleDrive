{
  "name": "teledrive",
  "version": "1.0.0",
  "description": "A file storage application that uses Telegram as a storage backend. This application allows you to store and manage files using your Telegram account, taking advantage of Telegram's generous storage capabilities.",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --detectOpenHandles",
    "test:unit": "jest --testMatch=\"**/*.unit.test.js\" --detectOpenHandles",
    "test:integration": "jest --testMatch=\"**/*.integration.test.js\" --detectOpenHandles",
    "sync": "node src/scripts/sync.js",
    "cleanup": "node src/scripts/cleanup.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "telegram",
    "storage",
    "file",
    "cloud",
    "drive"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "connect-mongo": "^4.6.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongoose": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "telegraf": "^4.11.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
